<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro">

  <xacro:property name="DEG2RAD" value="0.017453292519943295"/>

  <xacro:macro name="gp25" params="prefix attach">
    <!-- LINKS -->
    <!-- virtual link for interfacing with base-->
    <link name="${prefix}arm_base">
      <visual>
        <geometry>
          <box size="0.1 0.1 0.1"/>
        </geometry>
        <origin rpy="0 0 0" xyz="0 0 0"/>
      </visual>
    </link>
    <!-- Foundation, link 0, sometimes referred to as base_link-->
    <link name="${prefix}f_link">
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="/opt/dorabot/resources/arms/gp25/mesh/f-visual.stl" />
        </geometry>
        <material name="red">
          <color rgba="1.0 0 0 1.0"/>
        </material>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="/opt/dorabot/resources/arms/gp25/mesh/f-collision.stl" />
        </geometry>
      </collision>
    </link>
    <!-- Swing/Swivel , link 1 (A1) -->
    <link name="${prefix}s_link">
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="/opt/dorabot/resources/arms/gp25/mesh/s-visual.stl" />
        </geometry>
        <material name="blue">
          <color rgba="0 0 1.0 1.0"/>
        </material>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="/opt/dorabot/resources/arms/gp25/mesh/s-collision.stl" />
        </geometry>
      </collision>
    </link>
    <!-- Lower arm, link 2 -->
    <link name="${prefix}l_link">
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="/opt/dorabot/resources/arms/gp25/mesh/l-visual.stl"/>
        </geometry>
        <material name="red">
          <color rgba="1.0 0 0 1.0"/>
        </material>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="/opt/dorabot/resources/arms/gp25/mesh/l-collision.stl" />
        </geometry>
      </collision>
    </link>
    <!-- Upper arm, link 3 -->
    <link name="${prefix}u_link">
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="/opt/dorabot/resources/arms/gp25/mesh/u-visual.stl" />
        </geometry>
        <material name="blue">
          <color rgba="0 0 1.0 1.0"/>
        </material>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="/opt/dorabot/resources/arms/gp25/mesh/u-collision.stl" />
        </geometry>
      </collision>
    </link>
    <!-- arm Roll/wrist Rotation, link 4  -->
    <link name="${prefix}r_link">
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="/opt/dorabot/resources/arms/gp25/mesh/r-visual.stl" />
        </geometry>
        <material name="red">
          <color rgba="1.0 0 0 1.0"/>
        </material>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="/opt/dorabot/resources/arms/gp25/mesh/r-collision.stl" />
        </geometry>
      </collision>
    </link>
    <!-- wrist Bend, link 5 -->
    <link name="${prefix}b_link">
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="/opt/dorabot/resources/arms/gp25/mesh/b-visual.stl" />
        </geometry>
        <material name="blue">
          <color rgba="0 0 1.0 1.0"/>
        </material>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="/opt/dorabot/resources/arms/gp25/mesh/b-collision.stl" />
        </geometry>
      </collision>
    </link>
    <!-- Tool flange/wrist Turning, link 6 -->
    <link name="${prefix}t_link">
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="/opt/dorabot/resources/arms/gp25/mesh/t-visual.stl" />
        </geometry>
        <material name="red">
          <color rgba="1.0 0 0 1.0"/>
        </material>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="/opt/dorabot/resources/arms/gp25/mesh/t-collision.stl" />
        </geometry>
      </collision>
    </link>
    <!-- virtual link for interfacing with end effector-->
    <link name="${prefix}ee_mount">
      <visual>
        <geometry>
          <box size="0.01 0.01 0.01"/>
        </geometry>
        <origin rpy="0 0 0" xyz="0 0 0"/>
      </visual>
    </link>
    <!-- END LINKS -->

    <!-- JOINTS -->
    <!-- virtual joint -->
    <!-- virtual joint for interfacing with base -->
    <joint name="${prefix}arm_base_joint" type="fixed">
      <parent link="${attach}arm_mount"/>
      <child link="${prefix}arm_base"/>
      <origin xyz="0.0 0.0 0.0" rpy="0 0 0"/>
    </joint>
    <!-- virtual joint -->
    <joint name="${prefix}f_joint" type="fixed">
      <parent link="${prefix}arm_base"/>
      <child link="${prefix}f_link"/>
      <origin xyz="0.0 0.0 0.0" rpy="0 0 0"/>
    </joint>
    <!-- joint 1 (A1) -->
    <joint name="${prefix}s_joint" type="revolute">
      <origin xyz="0 0 0.169" rpy="0 0 0"/>
      <parent link="${prefix}f_link"/>
      <child link="${prefix}s_link"/>
      <axis xyz="0 0 1"/>
      <limit effort="0" lower="${-DEG2RAD * 180}" upper="${DEG2RAD * 180}" velocity="${DEG2RAD * 210}"/>
    </joint>
    <!-- joint 2 (A2) -->
    <joint name="${prefix}l_joint" type="revolute">
      <origin xyz="0.150 -0.112 0.336" rpy="0 0 0"/>
      <parent link="${prefix}s_link"/>
      <child link="${prefix}l_link"/>
      <axis xyz="0 1 0"/>
      <limit effort="0" lower="${-DEG2RAD * 105}" upper="${DEG2RAD * 155}" velocity="${DEG2RAD * 210}"/>
    </joint>
    <!-- joint 3 (A3) -->
    <joint name="${prefix}u_joint" type="revolute">
      <origin xyz="0 0.049 0.760" rpy="0 0 0"/>
      <parent link="${prefix}l_link"/>
      <child link="${prefix}u_link"/>
      <axis xyz="0 -1 0"/>
      <limit effort="0" lower="${-DEG2RAD * 86}" upper="${DEG2RAD * 160}" velocity="${DEG2RAD * 265}"/>
    </joint>
    <!-- joint 4 (A4) -->
    <joint name="${prefix}r_joint" type="revolute">
      <origin xyz="0.295 0.066 0.200" rpy="0 0 0"/>
      <parent link="${prefix}u_link"/>
      <child link="${prefix}r_link"/>
      <axis xyz="-1 0 0"/>
      <limit effort="0" lower="${-DEG2RAD * 200}" upper="${DEG2RAD * 200}" velocity="${DEG2RAD * 420}"/>
    </joint>
    <!-- joint 5 (A5) -->
    <joint name="${prefix}b_joint" type="revolute">
      <origin xyz="0.500 -0.063 0.0" rpy="0 0 0"/>
      <parent link="${prefix}r_link"/>
      <child link="${prefix}b_link"/>
      <axis xyz="0 -1 0"/>
      <limit effort="0" lower="${-DEG2RAD * 150}" upper="${DEG2RAD * 150}" velocity="${DEG2RAD * 420}"/>
    </joint>
    <!-- joint 6 (A6) -->
    <joint name="${prefix}t_joint" type="revolute">
      <origin xyz="0.100 0.060 0.0" rpy="0 0 0"/>
      <parent link="${prefix}b_link"/>
      <child link="${prefix}t_link"/>
      <axis xyz="-1 0 0"/>
      <limit effort="0" lower="${-DEG2RAD * 455}" upper="${DEG2RAD * 455}" velocity="${DEG2RAD * 885}"/>
    </joint>
   <!-- virtual joint for interfacing with end effector -->
    <joint name="${prefix}ee_mount_joint" type="fixed">
      <parent link="${prefix}t_link"/>
      <child link="${prefix}ee_mount"/>
      <origin xyz="0.0 0.0 0.0" rpy="0 0 0"/>
    </joint>
    <!-- END JOINTS -->
  </xacro:macro>
</robot>

